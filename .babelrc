{
  "presets": [
    "env",
    "stage-2",
    "react"
  ],
  "plugins": [
    "transform-decorators-legacy",
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }],
    ["import", {
      "libraryName": "antd",
      "style": "css"
    }]
  ]
}